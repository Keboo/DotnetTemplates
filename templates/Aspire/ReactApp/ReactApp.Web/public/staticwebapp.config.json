{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/pwa-*.png", "/favicon.ico", "/robots.txt", "/apple-touch-icon.png", "/sw.js", "/workbox-*.js", "/manifest.webmanifest"]
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".webmanifest": "application/manifest+json"
  }
}
